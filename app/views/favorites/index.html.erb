<!-- index.html.erb -->

<div class="favorite">
  <div class="headline"><h2><%= current_user.first_name %>'s Favorites</h2></div>
  <% @favorites.each do |favorite| %>
      <div class="headline"><h3><%=favorite.headline%></h3></div>
      <div class="lead_paragraph"><p><%=favorite.lead_paragraph%><p></div>
  <% end %>
</div>


  